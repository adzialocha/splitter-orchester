{
  "name": "splitter-orchester",
  "version": "0.1.0",
  "description": "Website for Splitter Orchester Berlin",
  "main": "index.js",
  "scripts": {
    "build": "concurrently npm:sanity:build npm:next:build",
    "lint": "eslint '*/**/*.{ts,tsx}'",
    "next:build": "next build",
    "next:start": "next dev",
    "next:start:vercel": "next --port $PORT",
    "next:telemetry": "next telemetry disable",
    "postinstall": "lerna bootstrap && npm run next:telemetry",
    "sanity:build": "cd studio && sanity build ../public/studio -y",
    "sanity:start": "cd studio && sanity start",
    "start": "concurrently npm:sanity:start npm:next:start",
    "vercel": "npm run vercel:deploy",
    "vercel:deploy": "vercel",
    "vercel:deploy:production": "vercel --prod",
    "vercel:dev": "concurrently npm:next:start:vercel npm:sanity:start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adzialocha/splitter-orchester.git"
  },
  "private": true,
  "author": "adzialocha",
  "license": "UNLICENSED",
  "homepage": "https://github.com/adzialocha/splitter-orchester#readme",
  "devDependencies": {
    "@sanity/cli": "^2.10.5",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.10",
    "@types/react-dom": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "autoprefixer": "^10.2.6",
    "concurrently": "^6.2.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "lerna": "^4.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.1",
    "tailwindcss": "^2.1.4",
    "typescript": "^4.3.2",
    "vercel": "^23.0.0"
  },
  "dependencies": {
    "@sanity/block-content-to-react": "^3.0.0",
    "date-fns": "^2.22.1",
    "next": "^10.2.3",
    "next-sanity": "^0.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
